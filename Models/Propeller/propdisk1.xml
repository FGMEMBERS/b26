<?xml version="1.0"?>

<PropertyList>

 <path>propdisk.ac</path>

  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>slowpdisk.R</object-name>
    <object-name>fastpdisk.R</object-name>
    <object-name>slowpdisk.F</object-name>
    <object-name>fastpdisk.F</object-name>
  </effect>

  <animation>
    <type>scale</type>
  <x-offset>2.0</x-offset>
  <y-offset>2.0</y-offset>
  <z-offset>2.0</z-offset>
 </animation>

<animation>
  <type>select</type>
  <object-name>fastpdisk.R</object-name>
  <object-name>fastpdisk.F</object-name>
  <condition>
   <greater-than>
    <property>engines/engine[1]/rpm</property>
    <value>1700</value>
   </greater-than>
   <not-equals>
    <property>engines/engine[1]/propstrike</property>
    <value>1</value>
   </not-equals>
  </condition>
 </animation>

<animation>
  <type>spin</type>
  <object-name>slowpdisk.R</object-name>
  <object-name>slowpdisk.F</object-name>
    <property>engines/engine[1]/rpm</property>
    <factor>0.35</factor>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>

<animation>
  <type>spin</type>
  <object-name>fastpdisk.R</object-name>
  <object-name>fastpdisk.F</object-name>
    <property>engines/engine[1]/rpm</property>
    <factor>0.1</factor>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>




 <animation>
  <type>select</type>
  <object-name>slowpdisk.F</object-name>
  <object-name>slowpdisk.R</object-name>
  <condition>
   <less-than>
    <property>engines/engine[1]/rpm</property>
    <value>1701</value>
   </less-than>
   <greater-than>
    <property>engines/engine[1]/rpm</property>
    <value>600</value>
   </greater-than>
   <not-equals>
    <property>engines/engine[1]/propstrike</property>
    <value>1</value>
   </not-equals>
  </condition>
 </animation>

 <animation>
  <type>blend</type>
  <object-name>slowpdisk.F</object-name>
  <object-name>slowpdisk.R</object-name>
  <property>engines/engine[1]/rpm</property>
  <factor>0.000588</factor>
  <min>0.0</min>
  <max>0.5</max>
 </animation>

 <animation>
  <type>blend</type>
  <object-name>fastpdisk.R</object-name>
  <object-name>fastpdisk.F</object-name>
  <property>engines/engine[1]/rpm</property>
  <factor>0.0003</factor>
  <min>0.0</min>
  <max>0.6</max>
 </animation>

</PropertyList>
