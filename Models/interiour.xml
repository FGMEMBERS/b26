<?xml version="1.0"?>

<PropertyList>
		<path>interiour.ac</path>
<!-- crew -->
    <model> 
      <path>Aircraft/mosquito/Models/pilot.xml</path>
	  <name>pilot</name>
      <offsets> 
        <x-m>2.9</x-m>
        <y-m>-0.38</y-m>
        <z-m>0.135</z-m>
        <pitch-deg>14</pitch-deg>
      </offsets>
    </model>
 <animation>
  <type>select</type>
  <object-name>pilot</object-name>
  <condition>
   <greater-than>
    <property>/sim/model/crew/pilot/visible</property>
    <value>0.0</value>
   </greater-than>
  </condition>
 </animation>

    <model> 
      <path>Aircraft/mosquito/Models/crew.xml</path>
	  <name>crew</name>
      <offsets> 
        <x-m>2.9</x-m>
        <y-m>0.38</y-m>
        <z-m>0.14</z-m>
        <pitch-deg>14</pitch-deg>
      </offsets>
    </model>
 <animation>
  <type>select</type>
  <object-name>crew</object-name>
  <condition>
   <less-than>
    <property>/sim/current-view/view-number</property>
    <value>6.0</value>
   </less-than>
  </condition>
 </animation>
 <model>
	<name>pilot chair</name>
  <path>Aircraft/b26/Models/Interiour/chair.ac</path>
  <offsets>
   <x-m>3.05</x-m>
   <y-m>-0.4</y-m>
   <z-m>-0.2</z-m>
  </offsets>
 </model>
 <model>
	<name>copilot chair</name>
  <path>Aircraft/b26/Models/Interiour/chair.ac</path>
  <offsets>
   <x-m>3.05</x-m>
   <y-m>0.52</y-m>
   <z-m>-0.2</z-m>
  </offsets>
 </model>
    <model> 
      <path>Aircraft/b26/Models/Interiour/nosegun.xml</path>
	  <name>nosegun</name>
      <offsets> 
        <x-m>0.06</x-m>
        <y-m>0.0</y-m>
        <z-m>-0.27</z-m>
        <pitch-deg>0</pitch-deg>
      </offsets>
    </model>
    <model> 
      <path>Aircraft/b26/Models/Interiour/turretgun.xml</path>
	  <name>turretgun</name>
      <offsets> 
        <x-m>12.05</x-m>
        <y-m>0.0</y-m>
        <z-m>1.0</z-m>
        <pitch-deg>0</pitch-deg>
				<heading-deg>180</heading-deg>
      </offsets>
    </model>

    <model> 
      <path>Aircraft/b26/Models/Interiour/browning50fsl.L.xml</path>
	  <name>reargun left</name>
      <offsets> 
        <x-m>13.35</x-m>
        <y-m>-0.4</y-m>
        <z-m>-0.32</z-m>
        <pitch-deg>0</pitch-deg>
	<heading-deg>90</heading-deg>
      </offsets>
    </model>

    <model> 
      <path>Aircraft/b26/Models/Interiour/browning50fsl.L.xml</path>
	  <name>reargun right</name>
      <offsets> 
        <x-m>13.35</x-m>
        <y-m>0.4</y-m>
        <z-m>-0.32</z-m>
        <pitch-deg>0</pitch-deg>
	<heading-deg>-90</heading-deg>
      </offsets>
    </model>

  <animation>
    <type>pick</type>
    <object-name>reargunL.int</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>nasal</command>
        <script>b26.reardoorL.toggle();</script>
      </binding>
    </action>
  </animation>

 <animation>
  <type>translate</type>
  <object-name>reargunL.int</object-name>
  <property>controls/doors/reardoor.L/position-norm</property>
  <interpolation>
   <entry>
    <ind>0.0</ind>
    <dep>0.0</dep>
   </entry>
   <entry>
    <ind>0.2</ind>
    <dep>0.09</dep>
   </entry>
   <entry>
    <ind>1.00</ind>
    <dep>0.09</dep>
   </entry>
  </interpolation>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.45</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>reargunL.int</object-name>
  <property>controls/doors/reardoor.L/position-norm</property>
  <interpolation>
   <entry>
    <ind>0.0</ind>
    <dep>0.0</dep>
   </entry>
   <entry>
    <ind>0.2</ind>
    <dep>0.0</dep>
   </entry>
  <entry>
    <ind>1.00</ind>
    <dep>55.0</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>13.46</x-m>
   <y-m>0.2</y-m>
   <z-m>0.18</z-m>
  </center>
  <axis>
   <x>-1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>translate</type>
  <object-name>reargunR.int</object-name>
  <property>controls/doors/cdoor/position-norm</property>
  <interpolation>
   <entry>
    <ind>0.0</ind>
    <dep>0.0</dep>
   </entry>
   <entry>
    <ind>0.2</ind>
    <dep>-0.09</dep>
   </entry>
   <entry>
    <ind>1.00</ind>
    <dep>-0.09</dep>
   </entry>
  </interpolation>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.45</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>reargunR.int</object-name>
  <property>controls/doors/reardoor.L/position-norm</property>
  <interpolation>
   <entry>
    <ind>0.0</ind>
    <dep>0.0</dep>
   </entry>
   <entry>
    <ind>0.2</ind>
    <dep>0.0</dep>
   </entry>
  <entry>
    <ind>1.00</ind>
    <dep>55.0</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>13.46</x-m>
   <y-m>-0.18</y-m>
   <z-m>0.2</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>select</type>
  <object-name>pilot</object-name>
  <condition>
   <greater-than>
    <property>/sim/current-view/view-number</property>
    <value>0.0</value>
   </greater-than>
  </condition>
 </animation>

</PropertyList>
