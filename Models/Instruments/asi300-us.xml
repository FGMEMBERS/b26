<?xml version="1.0"?>

<!-- air speed indicator -->

<PropertyList>

 <animation>
  <type>material</type>
  <object-name>case0</object-name>
  <object-name>case1</object-name>
  <object-name>screws</object-name>
  <emission>
    <factor-prop>/controls/lighting/instruments-norm</factor-prop>
    <red>0.2</red>
    <green>0.05</green>
    <blue>0.05</blue>
  </emission>
 </animation>
 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>needle1</object-name>
  <emission>
    <factor-prop>/controls/lighting/instruments-norm</factor-prop>
    <red>0.5</red>
    <green>0.25</green>
    <blue>0.25</blue>
  </emission>
 </animation>


	<path>asi300-us.ac</path>

	<animation>
		<type>rotate</type>
		<object-name>needle1</object-name>
		<property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>40</ind>
				<dep>12.8</dep>
			</entry>
			<entry>
				<ind>60</ind>
				<dep>34.5</dep>
			</entry>
			<entry>
				<ind>80</ind>
				<dep>57.5</dep>
			</entry>
			<entry>
				<ind>100</ind>
				<dep>93.5</dep>
			</entry>
			<entry>
				<ind>120</ind>
				<dep>128</dep>
			</entry>
			<entry>
				<ind>140</ind>
				<dep>162</dep>
			</entry>
			<entry>
				<ind>160</ind>
				<dep>204</dep>
			</entry>
			<entry>
				<ind>180</ind>
				<dep>239</dep>
			</entry>
			<entry>
				<ind>200</ind>
				<dep>274.5</dep>
			</entry>
			<entry>
				<ind>250</ind>
				<dep>311</dep>
			</entry>
			<entry>
				<ind>300</ind>
				<dep>349</dep>
			</entry>

		</interpolation>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

</PropertyList>
