    <value>3</value>
   </equals>
  </condition>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>selector</object-name>
  <object-name>swup</object-name>
  <object-name>swdown</object-name>
  <property>controls/fuel/indicator/tank-selected</property>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>-45</dep>
   </entry>
   <entry>
    <ind>1</ind>
    <dep>45</dep>
   </entry>
   <entry>
    <ind>2</ind>
    <dep>135</dep>
   </entry>
   <entry>
    <ind>3</ind>
    <dep>225</dep>
   </entry>

  </interpolation>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>-0.042</z-m>
  </center>
 </animation>

  <animation>
    <type>pick</type>
    <object-name>swup</object-name>
    <visible>false</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-adjust</command>
  <property>controls/fuel/indicator/tank-selected</property>
        <step>1</step>
        <min>0</min>
        <max>4</max>
        <wrap>1</wrap>
      </binding>
    </action>
 </animation>

  <animation>
    <type>pick</type>
    <object-name>swdown</object-name>
    <visible>false</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-adjust</command>
  <property>controls/fuel/indicator/tank-selected</property>
        <step>-1</step>
        <min>0</min>
        <max>4</max>
        <wrap>1</wrap>
      </binding>
    </action>
 </animation>

</PropertyList>


