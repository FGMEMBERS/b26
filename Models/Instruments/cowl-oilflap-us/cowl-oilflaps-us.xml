<?xml version="1.0"?>
<PropertyList>

 <path>cowl-oilflaps-us.ac</path>

 <animation>
  <type>material</type>
  <object-name>case0</object-name>
  <object-name>case1</object-name>
  <object-name>screws</object-name>
  <emission>
    <factor-prop>/controls/lighting/instruments-norm</factor-prop>
    <red>0.2</red>
    <green>0.05</green>
    <blue>0.05</blue>
  </emission>
 </animation>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>needle1</object-name>
  <object-name>needle2</object-name>
  <emission>
    <factor-prop>/controls/lighting/instruments-norm</factor-prop>
    <red>0.5</red>
    <green>0.25</green>
    <blue>0.25</blue>
  </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>needle1</object-name>
  <property>/engines/engine/cylinder-temp-degc</property>
  <min>-100</min>
  <max>300</max>
  <interpolation>
   <entry>
    <ind>-100</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>0</ind>
    <dep>29</dep>
   </entry>
   <entry>
    <ind>100</ind>
    <dep>58</dep>
   </entry>
   <entry>
    <ind>200</ind>
    <dep>88.5</dep>
   </entry>
   <entry>
    <ind>300</ind>
    <dep>117.0</dep>
   </entry>
  </interpolation>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
   <x-m>0.0</x-m>
   <y-m>-0.027</y-m>
   <z-m>-0.0</z-m>
  </center>
 </animation>
 <animation>
  <type>rotate</type>
  <object-name>needle2</object-name>
  <property>/engines/engine[1]/cylinder-temp-degc</property>
  <min>-100</min>
  <max>300</max>
  <interpolation>
   <entry>
    <ind>-100</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>0</ind>
    <dep>29</dep>
   </entry>
   <entry>
    <ind>100</ind>
    <dep>58</dep>
   </entry>
   <entry>
    <ind>200</ind>
    <dep>88.5</dep>
   </entry>
   <entry>
    <ind>300</ind>
    <dep>117.0</dep>
   </entry>
  </interpolation>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.027</y-m>
   <z-m>-0.0</z-m>
  </center>
 </animation>

</PropertyList>


